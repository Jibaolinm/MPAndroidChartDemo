<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.petterp.guosai">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".ShujuFenxi.ShujuActivity" />
        <activity android:name=".Main2Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Environment.Environment" />
        <activity android:name=".Environment.RealTime" />
        <activity android:name=".LifeZhushou.LifeActivity" />
        <activity android:name=".GuosaiTest.Weather.WeatherActivity" />
        <activity android:name=".GuosaiTest.WeiZhang.WeiZhangActivity" />
        <activity android:name=".Test">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
                <category android:name="android.intent.category.BROWSABLE"></category>
                <data
                    android:scheme="qwe"
                    android:host="www.baidu.com">
                </data>
            </intent-filter>

        </activity>
    </application>

</manifest>